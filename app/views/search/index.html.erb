<% @house_members.each do |member| %>
  <h1> <%= "#{member.house} (#{member.count} Members)" %> </h1>
    <ul class='house-member'>
      <li class='name'> <%= member.name %> </li>
      <li class='role'> <%= member.role %> </li>
      <li class='house'> <%= member.house %> </li>
      <li class='patronus'> <%= member.patronus %> </li>
    </ul>
<% end %>
